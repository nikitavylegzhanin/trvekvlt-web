{
  "name": "trvekvlt-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "3.6.9",
    "date-fns": "2.29.1",
    "graphql.macro": "1.4.2",
    "react": "18.2.0",
    "react-day-picker": "8.0.7",
    "react-dom": "18.2.0",
    "react-google-charts": "4.0.0",
    "styled-components": "5.3.5",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@types/node": "16.11.46",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.25",
    "graphql": "16.5.0",
    "react-scripts": "5.0.1",
    "typescript": "4.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "packageRules": [
      {
        "matchUpdateTypes": [
          "minor",
          "patch",
          "pin",
          "digest"
        ],
        "automerge": true
      }
    ]
  },
  "os": [
    "darwin",
    "linux"
  ]
}
